{
  "extends": ["next/core-web-vitals"],
  "ignorePatterns": [
    "src/generated/**/*",
    "prisma/generated/**/*",
    "**/generated/**/*"
  ],
  "overrides": [
    {
      "files": ["**/generated/**/*.js", "src/generated/**/*.js"],
      "rules": {
        "@typescript-eslint/no-require-imports": "off",
        "import/no-commonjs": "off",
        "@typescript-eslint/no-var-requires": "off"
      }
    }
  ]
}
